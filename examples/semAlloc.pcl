program hello;
    var ptr : ^integer;
    var i : integer;
begin
    i := 0;

    while i < 2048 do
    begin
        new ptr;
        if ptr = nil then
            writeString("Achieved nullptr\n");
        ptr^ := i;
        dispose ptr;
        i := i + 1;
    end;

    writeString("done\n");

end.